#include <stdio.h>

void main(void) {
  int Peso;
  float Altura;
  float IMC;

  printf("Digite o Seu Peso: ");
  scanf("%d", &Peso);
  printf("Digite a sua Altura: ");
  scanf("%f", &Altura);
  
IMC = Peso / (Altura * Altura);
printf("IMC: %f\n", IMC);
  
  if( IMC >= 18.5 < 24.9 ) {
    puts("Intervalo normal");
  } else {
    puts("Fora do intervalo normal");
  }
  
}
